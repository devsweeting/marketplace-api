{
  "name": "marketplace",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "fixtures": "node  dist/config/database/index.config.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./test/jest.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "bundle": "rimraf public/*.{js,map} && parcel build src/modules/admin/pages/*.html --dist-dir public",
    "bundle:dev": "parcel src/modules/admin/pages/*.html --dist-dir public",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:create": "sh -c 'yarn typeorm migration:create ./src/config/database/migrations/$0'",
    "migration:generate": "sh -c 'yarn typeorm migration:generate -d ./src/config/database/cli-data-source.ts ./src/config/database/migrations/$0'",
    "migration:up": "yarn typeorm migration:run -d ./src/config/database/cli-data-source.ts",
    "migration:down": "yarn typeorm migration:revert -d ./src/config/database/cli-data-source.ts",
    "migration:show": "yarn typeorm migration:show -d ./src/config/database/cli-data-source.ts"
  },
  "dependencies": {
    "@adminjs/design-system": "^2.2.4",
    "@adminjs/express": "^4.0.3",
    "@adminjs/logger": "^2.2.0",
    "@adminjs/typeorm": "^3.0.2",
    "@adminjs/upload": "^2.0.2",
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/terminus": "^8.0.6",
    "@nestjs/typeorm": "^9.0.1",
    "@ntegral/nestjs-sentry": "^3.0.7",
    "@sentry/node": "^7.8.1",
    "@sentry/tracing": "^7.8.1",
    "adminjs": "^5.10.3",
    "aws-sdk": "^2.1095.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "class-sanitizer": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-fns": "^2.28.0",
    "ethereumjs-util": "^7.1.4",
    "ethers": "^5.6.1",
    "express": "^4.17.3",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.2",
    "handlebars": "^4.7.7",
    "hashids": "^2.2.10",
    "hbs": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "mime-types": "^2.1.35",
    "nestjs-rollbar": "^1.7.0",
    "nestjs-typeorm-paginate": "^4.0.2",
    "nodemailer": "^6.7.2",
    "passport": "^0.5.2",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.6.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.2.0",
    "typeorm": "^0.3.9",
    "typeorm-store": "^2.0.0",
    "uuid": "^8.3.2"
